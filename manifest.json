{
  "manifest_version": 3,
  "name": "Application tracking",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "permissions": ["activeTab", "storage", "downloads"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/scripts/readPageData.ts"]
    }
  ]
}
